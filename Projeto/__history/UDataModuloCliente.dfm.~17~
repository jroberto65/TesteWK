  object fdQryPesquisaCliente: TFDQuery
    Connection = DataModulo.fdConexao
    SQL.Strings = (
      'Select * from Clientes'
      'Where nome like('#39'%'#39'+ :nome +'#39'%'#39')')
    Left = 240
    Top = 72
    ParamData = <
      item
        Name = 'NOME'
        DataType = ftString
        ParamType = ptInput
      end>
    object fdQryPesquisaClienteCodigo: TFDAutoIncField
      FieldName = 'Codigo'
      Origin = 'Codigo'
      ProviderFlags = [pfInWhere, pfInKey]
    end
    object fdQryPesquisaClienteNome: TStringField
      FieldName = 'Nome'
      Origin = 'Nome'
      Required = True
      Size = 250
    end
    object fdQryPesquisaClienteCidade: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'Cidade'
      Origin = 'Cidade'
